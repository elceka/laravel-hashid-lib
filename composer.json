{
    "name": "elceka/laravel-hashid-lib",
    "description": "Implementation of Hashids library for Laravel projects.",
    "type": "library",
    "require": {
        "php": "^7.4",
        "hashids/hashids": "^4.0"
    },
    "require-dev": {
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Elceka\\Laravel\\HashId\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elceka\\Laravel\\HashId\\LaravelHashidsServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslav Lukac",
            "email": "lukac@elceka.sk"
        }
    ],
    "minimum-stability": "dev"
}
